{
  "name": "shared-assets-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all --parallel build:*",
    "build:js": "browserify src/index.js -o dist/index.js",
    "build:html": "cp src/index.html dist/index.html",
    "build:css": "rsync node_modules/tachyons/css/tachyons.min.css ./dist",
    "build:mapbox-gl": "rsync -r node_modules/mapbox-gl/dist/* ./dist/mapbox-gl",
    "build:mapbox-gl-geocoder": "rsync -r node_modules/mapbox-gl-geocoder/dist/* ./dist/mapbox-gl-geocoder",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "watchify src/index.js -o dist/index.js",
    "watch:html": "nodemon --exec 'npm run build:html' --watch src/index.html",
    "watch:server": "http-server dist -c-1 -p 3000",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "TABLEFLIP",
  "license": "ISC",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react",
            "stage-2"
          ]
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "leaflet": "^1.0.1",
    "mapbox": "^1.0.0-beta3",
    "mapbox-gl-geocoder": "git://github.com/tableflip/mapbox-gl-geocoder.git#fix-import",
    "query-string": "^4.2.3",
    "raster-tile-style": "^1.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-geocoder": "^2.5.0",
    "react-leaflet": "^0.12.3",
    "react-map-gl": "^1.6.1",
    "tachyons": "^4.5.3",
    "underscore.string": "^3.3.4",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^3.1.0",
    "tachyons-cli": "^1.0.8",
    "watchify": "^3.7.0"
  }
}
